pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git 'https://github.com/Veerabommunikhil/hello-world.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
        stage('test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}        
