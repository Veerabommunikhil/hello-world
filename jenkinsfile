pipeline{
    tools{
    maven 'maven'
    }
    stages {
        stage("clone") {
            step {
                git 'https://github.com/Veerabommunikhil/hello-world.git'
            }
        stage("build") {
            step {
                sh 'mvn package'
            }
        stage("test") {
            step {
                sh 'mvn test'
            }
        }
        }
        }
    }
}
