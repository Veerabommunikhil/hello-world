pipeline{
    agent any

    tools{
    maven 'maven'
    }

    stages {
        stages('test'){
            steps {
                git 'https://github.com/Veerabommunikhil/hello-world.git'
            }

        stage('build'){
            steps {
                sh 'mvn package'
            }
            
        stage('test'){
            steps {
                sh 'mvn test'
            }
        }
        }
        }
    }
}
